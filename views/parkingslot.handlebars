{{> navbar-user }}

 {{> add-availability-modal }}




  <div class="account wf-section">
    <h3 class="heading">Parking Slot</h3><img src="/images/default-user-image.png" loading="lazy" width="150" height="150" alt="" class="account-image parking-slot">

    <form id="parkingslot-pic-upload" method="post" action="/api/parkingslotpic" enctype="multipart/form-data">
    <label>Upload parking slot picture</label>
    <input type="file" name="file" accept="image/jpeg"/>
    <input type="submit" value="update" name="submit"/>
    </form>

    <div class="profile">

        <div class="w-layout-grid profile-info" id="parkingslotDetails">
          <div>Parking Slot ID</div>
          <div>{{id}}</div>
          <div>Location</div>
          <div>{{carpark_building}}, {{carpark_area}}, {{carpark_district}}</div>
          <div>Floor</div>
          <div>{{level}}</div>
          <div>Unit</div>
          <div>{{unit}}</div>
        </div>
      

      <div class="w-form">
        <div>Description</div>
        <form id="email-form" name="email-form" data-name="Email Form" method="get"><textarea placeholder="{{description}}" maxlength="5000" id="Desciption" name="Desciption" data-name="field" class="textarea w-input"></textarea></form>
        <div class="w-form-done"></div>
        <div class="w-form-fail"></div>
      </div>
    </div>
  </div>
  <div class="section-4 wf-section">

    <h4 class="heading">Availability</h4>

    <div class="column-heading w-row">
      <div class="w-col w-col-3">
        <h5 class="box-item">Date</h5>
      </div>
      <div class="w-col w-col-3">
        <h5 class="box-item">Start</h5>
      </div>
      <div class="w-col w-col-3">
        <h5 class="box-item">End</h5>
      </div>
      <div class="w-col w-col-3 ">
        <button id="open-form"><i class="fas fa-plus-square"></i> add new availability</button>
      </div>
    </div>
    <div class="column-content w-row" id="available-table">





    </div>
  </div>

  {{>script}}
<script src="/js/parkingslot.js"></script>